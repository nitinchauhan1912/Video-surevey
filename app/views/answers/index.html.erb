<div class="responses_list">
  <div style="font-size:22px;margin:10px;">Responses - XXXX</div>
  
  <div style="border: 1px solid #2E2F30;width: 100%;height:500px">
    <div style="float: left;border-bottom: 1px solid #2E2F30;margin: 10px;width: auto">Date</div>
    <div style="float: left;border-bottom: 1px solid #2E2F30;margin: 10px;">Name</div>
    <div style="float: left;border-bottom: 1px solid #2E2F30;margin: 10px;">Email</div>
    
    <% @video.questions.where("question_type <> ? ",'email').order('id ASC').each do |q|  %>
      <div style="float: left;border-bottom: 1px solid #2E2F30;margin: 10px;">
          <%= q.question_type.titleize+' Question' %>  - <%= q.question_label %>
      </div>
    <% end %>
    
    
    
    
  </div>
  
  
</div>